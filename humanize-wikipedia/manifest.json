{
    "manifest_version": 2,

    "name": "Humanize Wikipedia",
    "description": "",
    "version": "0.0.1",

    "content_scripts": [
        {
            "matches": ["*://*.wikipedia.org/*"],
            "js": ["jquery.js", "humanize.js"],
            "all_frames": true,
            "run_at": "document_idle"
        }
    ]
}
